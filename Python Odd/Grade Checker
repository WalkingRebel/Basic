score = int(input("Enter your score:"))
if score > 98:
    grade = 'A+'
elif score >= 90 and score < 98:
    grade = 'A'
elif score >= 80:
    grade = 'B'
elif score >= 70:
    grade = 'C'
elif score >= 60:
    grade = 'C-'
elif score == 50:
    grade = 'D'
elif score <= 49:
    grade = 'F'
print(f"Your grade is: {grade}")